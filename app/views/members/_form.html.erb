
  <%= simple_form_for(@member, :html => {:class => "form-horizontal" }) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :phone %>
      <%= f.input :email %>
      <%= f.input :address %>
    </div>

  </div>
    <div class="btns">
        <%= f.button :submit, :class => "btn btn-primary pull-right"  %>
    </div>
<% end %>
