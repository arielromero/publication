
<div class="panel panel-info">
  <div class="panel-heading">
    <h4><%= t('member.index.title')%></h4>
    <form id="search_members_form" action="<%= search_members_url() %>" method="get" >
      <%= text_field_tag 'search_param' %>
    
      <button type="submit" class="btn btn-primary">
        <i class="glyphicon glyphicon-search "></i> <%= t('action.search')%>
      </button><br>
      <h4 id="error"></h4>
       <%#= link_to "Ver pendientes de entrega", backordered_members_path %>
    </form>
  </div>
  <div class="panel-body">
    <div id="members_search_results">
      <%= render 'members/list' %>
    </div>
  </div>
  <div class="panel-footer">
    <%= link_to t('member.new.caption'), new_member_path, :class => "btn btn-primary"  %>
  </div>
</div>



