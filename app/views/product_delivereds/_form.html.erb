<%= simple_form_for(@product_delivered) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :subscription_id %>
    <%= f.association :product_received,  :collection => @campaign.product_receiveds %>
    <%= f.input :product_received_id %>
    <%= f.input :delivered_at %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
